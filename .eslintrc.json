{
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
        "no-unused-vars" : 2,
        "no-undef" : 2
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx":  true,
            "modules": true,
            "spread" : true,
            "restParams" : true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env" : {
        "browser" : true,
        "node" : true,
        "es6" : true
    }
}